{
  "name": "meteora-invent",
  "version": "0.0.0",
  "private": true,
  "description": "Meteora Invent - Monorepo for studio scripts and scaffold templates",
  "author": "Dann Wee <dann@raccoons.dev>",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "turbo build --filter=!@meteora-invent/scaffold/fun-launch",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "clean": "turbo clean && rm -rf node_modules",
    "dbc-claim-all-fees": "pnpm --filter @meteora-invent/studio dbc-claim-all-fees",
    "studio": "pnpm --filter @meteora-invent/studio",
    "scaffold": "pnpm --filter '@meteora-invent/scaffold-*'",
    "validate-env": "turbo validate-env",
    "studio:damm-v1": "pnpm --filter @meteora-invent/studio run damm-v1:*",
    "studio:damm-v2": "pnpm --filter @meteora-invent/studio run damm-v2:*",
    "studio:dlmm": "pnpm --filter @meteora-invent/studio run dlmm:*",
    "studio:dbc": "pnpm --filter @meteora-invent/studio run dbc:*",
    "studio:dbc-claim-all-fees": "pnpm --filter @meteora-invent/studio run dbc-claim-all-fees -- --config ./studio/config/dbc_config.jsonc",
    "studio:dbc-claim-all-fees-auto": "pnpm --filter @meteora-invent/studio dbc-claim-all-fees-auto",
    "prepare": "husky install",
    "syncpack:check": "syncpack list-mismatches",
    "syncpack:fix": "syncpack fix-mismatches"
  },
  "devDependencies": {
    "@meteora-invent/config-prettier": "workspace:*",
    "@meteora-invent/config-typescript": "workspace:*",
    "@types/node": "^20.19.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.5.3",
    "syncpack": "^12.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@meteora-ag/alpha-vault": "^1.1.14",
    "@meteora-ag/cp-amm-sdk": "^1.1.0",
    "@meteora-ag/dlmm": "^1.6.1",
    "@meteora-ag/dynamic-amm-sdk": "^1.3.8",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.3.5",
    "@meteora-ag/m3m3": "^1.0.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.2",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react": "^18.3.23",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.6.1",
    "jotai": "^2.12.5",
    "js-sha256": "^0.11.1",
    "jsonc-parser": "^3.3.1",
    "ky": "^1.8.1",
    "next": "^14.2.31",
    "next-font": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.6.0",
    "sonner": "^2.0.6",
    "superstruct": "^2.0.2",
    "tailwind-merge": "^3.3.1",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.25.74"
  }
}

#############################################
# therebelion Monorepo Environment Example  #
#############################################
# Copy to .env.local (Next.js dev) and/or .env for scripts. Never commit secrets.

############################
# RPC / Network
############################
# Public RPC endpoint (used client-side). Provide a non-empty placeholder to satisfy CI env guard.
RPC_URL=https://api.mainnet-beta.solana.com
# Optional alternative variable names consumed by runtime (left blank by default)
RPC_ENDPOINT=
NEXT_PUBLIC_RPC_URL=
# Prefer NAME path; weâ€™ll auto-detect role at runtime:
DBC_CLAIM_FEE_INSTRUCTION_NAME="auto"            # "auto" means: try creator, fall back to partner
DBC_USE_IDL=true
# Optional if you insist on discriminator (else omit):
# DBC_CLAIM_FEE_DISCRIMINATOR=""
ALLOWED_DBC_PROGRAM_IDS=["dbcij3LWUppWqq96dh6gJWwBifmcGfLSB5D4DuSMaqN"]
ALLOWED_DAMM_V2_PROGRAM_IDS=["cpamdpZCGKUy5JxQXB4dcpGPiikHawvSWAd6mEn1sGG"]
# Optional while env-driven filters exist:
# MIGRATED_DBC_POOLS='["<poolPubkey1>","<poolPubkey2>"]'

############################
# Priority & Performance (optional tunables)
############################
# Default microLamports per compute unit for exits if UI value absent.
DEFAULT_PRIORITY_MICROS=250000
# Optional compute unit limit for fast mode (empty = none)
DEFAULT_COMPUTE_UNIT_LIMIT=

############################
# Misc / Diagnostics
############################
LOG_LEVEL=info
COMMITMENT=confirmed

# End of file
